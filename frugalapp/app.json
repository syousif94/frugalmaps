{
  "expo": {
    "name": "Lit Cal",
    "description": "LitCal lists nearby brunches, happy hours, and daily specials.",
    "slug": "litcal",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "facebookAppId": "1688723777922359",
    "facebookDisplayName": "LitCal",
    "updates": {
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "ON_ERROR_RECOVERY"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "me.syousif.LitCal"
    },
    "android": {
      "package": "me.syousif.LitCal"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#000"
    }
  }
}
