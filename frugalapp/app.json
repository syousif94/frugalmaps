{
  "expo": {
    "name": "Buncha",
    "description": "Buncha is a calendar of fun and affordable stuff to do, both every where and nearby.",
    "slug": "litcal",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.12",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "notification": {
      "icon": "./assets/notification.png",
      "color": "#000000"
    },
    "scheme": "buncha",
    "facebookAppId": "1688723777922359",
    "facebookDisplayName": "LitCal",
    "updates": {
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "ON_ERROR_RECOVERY"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "me.syousif.LitCal",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Location access is used to sort events and search results by distance and notify you when new events are added nearby",
        "NSCalendarsUsageDescription": "Allow to view events in your calendar"
      }
    },
    "android": {
      "package": "me.syousif.LitCal",
      "adaptiveIcon": {
        "backgroundColor": "#FF94CD",
        "foregroundImage": "./assets/foreground.png"
      },
      "versionCode": 2,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "VIBRATE",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "INTERNET",
        "ACCESS_NOTIFICATION_POLICY"
      ]
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#000"
    }
  }
}
