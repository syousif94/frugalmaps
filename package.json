{
  "name": "Buncha",
  "description": "A calendar of fun and affordable things to do nearby",
  "scripts": {
    "release": "cd frugalapp && expo publish",
    "ios": "cd frugalapp && expo build:ios",
    "android": "cd frugalapp && expo build:android",
    "start": "cd frugalapp && expo start",
    "deploy": "chmod a+x ./scripts/deploy.sh && ./scripts/deploy.sh",
    "server": "cd functions && npm run serve",
    "logs": "cd functions && npm run logs",
    "web": "firebase deploy --only hosting",
    "elastic": "cd functions && npm run elastic"
  },
  "private": true
}
