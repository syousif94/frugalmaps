{
  "name": "Buncha",
  "description": "A calendar of fun and affordable things to do nearby",
  "scripts": {
    "release": "cd buncha && expo publish",
    "ios": "cd buncha && expo build:ios",
    "android": "cd buncha && expo build:android",
    "start": "cd frugalapp && expo start",
    "test:api": "chmod a+x ./scripts/test-api.sh && ./scripts/test-api.sh",
    "deploy:api": "chmod a+x ./scripts/deploy-api.sh && ./scripts/deploy-api.sh",
    "server": "cd functions && npm run serve",
    "logs": "cd functions && npm run logs",
    "web": "chmod a+x ./scripts/deploy-web.sh && ./scripts/deploy-web.sh",
    "elastic": "cd functions && npm run elastic"
  },
  "private": true
}
